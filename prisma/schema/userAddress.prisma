model UserAddress {
  id                    Int             @id @default(autoincrement())
  address               String
  rt                    Int
  rw                    Int
  district              String
  city                  String
  province              String
  userId                String          @db.Uuid
  user                  User            @relation("UserAddresses", fields: [userId], references: [id])
  createdAt             DateTime        @default(now())
  updatedAt             DateTime        @updatedAt
}
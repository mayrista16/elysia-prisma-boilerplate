model Cars {
  id                    Int             @id @default(autoincrement())
  name                  String
  brand                 String
  release_year          Int
  plate_number          String          @unique
  status                String
  user_car              UserCar[]       @relation("UserCarsfk")
  createdAt             DateTime        @default(now())
  updatedAt             DateTime        @updatedAt
}